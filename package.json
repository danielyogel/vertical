    "react-is": "^17.0.1",
    "size-limit": "^4.9.1",
    "ts-toolbelt": "8.0.7",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@types/lodash": "^4.14.168",
