{
  "name": "@daniely/vertical",
  "description": "Vertical Abstract Software",
  "author": "Daniel Yogel",
  "version": "0.8.3",
  "license": "MIT",
  "main": "./dist/vertical.umd.js",
  "module": "./dist/vertical.es.js",
  "exports": {
    ".": {
      "import": "./dist/vertical.es.js",
      "require": "./dist/vertical.umd.js"
    }
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "concurrently  --group  --raw \"vite\" \"tsc --noEmit --watch\" \"npm run ladle\" \"npm run test\"",
    "build": "concurrently --raw \"vite build\" \"npm run build:types\"",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build",
    "ladle": "npx ladle serve",
    "ladle:build": "npx ladle build --out dist-ladle",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "engines": {
    "node": ">=12"
  },
  "prettier": {
    "printWidth": 170,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@ladle/react": "^2.17.2",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.4.10",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "antd": "^5.8.3",
    "classnames": "^2.3.2",
    "concurrently": "^8.2.0",
    "postcss": "^8.4.27",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "ts-toolbelt": "9.6.0",
    "tslib": "^2.6.1",
    "type-fest": "^4.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^0.34.1"
  },
  "dependencies": {
    "fp-ts": "^2.16.1",
    "io-ts": "^2.2.20",
    "lodash": "^4.17.21",
    "mobx": "^6.10.0",
    "mobx-react-lite": "^4.0.3",
    "nanoid": "^4.0.2",
    "tsd": "^0.28.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielyogel/vertical.git"
  },
  "bugs": {
    "url": "https://github.com/danielyogel/vertical/issues"
  },
  "homepage": "https://github.com/danielyogel/vertical#readme"
}
