{
  "name": "@daniely/vertical",
  "description": "Vertical Abstract Software",
  "author": "Daniel Yogel",
  "version": "0.8.1",
  "license": "MIT",
  "main": "./dist/vertical.umd.js",
  "module": "./dist/vertical.es.js",
  "exports": {
    ".": {
      "import": "./dist/vertical.es.js",
      "require": "./dist/vertical.umd.js"
    }
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "concurrently --raw \"vite\" \"tsc --noEmit --watch\" \"npm run storybook\"",
    "build": "concurrently --raw \"vite build\" \"npm run build:types\"",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "engines": {
    "node": ">=12"
  },
  "prettier": {
    "printWidth": 150,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "peerDependencies": {
    "react": ">=16",
    "mobx": "^6.0.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addons": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "storybook-builder-vite": "^0.1.19",
    "@types/lodash": "^4.14.176",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "antd": "^4.16.13",
    "classnames": "^2.3.1",
    "postcss": "^8.3.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "ts-toolbelt": "9.6.0",
    "type-fest": "2.12.0",
    "concurrently": "7.0.0"
  },
  "dependencies": {
    "fp-ts": "2.11.9",
    "io-ts": "^2.2.16",
    "lodash": "^4.17.21",
    "mobx": "^6.3.6",
    "mobx-react-lite": "^3.2.2",
    "nanoid": "^3.1.30"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielyogel/vertical.git"
  },
  "bugs": {
    "url": "https://github.com/danielyogel/vertical/issues"
  },
  "homepage": "https://github.com/danielyogel/vertical#readme"
}
