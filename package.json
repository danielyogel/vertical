{
  "name": "@daniely/vertical",
  "description": "Vertical Abstract Software",
  "author": "Daniel Yogel",
  "version": "0.8.3",
  "license": "MIT",
  "main": "./dist/vertical.umd.js",
  "module": "./dist/vertical.es.js",
  "exports": {
    ".": {
      "import": "./dist/vertical.es.js",
      "require": "./dist/vertical.umd.js"
    }
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "concurrently  --group  --raw \"vite\" \"tsc --noEmit --watch\" \"npm run ladle\" \"npm run test\"",
    "build": "concurrently --raw \"vite build\" \"npm run build:types\"",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build",
    "ladle": "npx ladle serve",
    "ladle:build": "npx ladle build --out dist-ladle",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "engines": {
    "node": ">=12"
  },
  "prettier": {
    "printWidth": 170,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@ladle/react": "^2.12.3",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "antd": "^5.4.6",
    "classnames": "^2.3.2",
    "concurrently": "^8.0.1",
    "postcss": "^8.4.23",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "ts-toolbelt": "9.6.0",
    "tslib": "^2.5.0",
    "type-fest": "^3.9.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.4",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "fp-ts": "^2.14.0",
    "io-ts": "^2.2.20",
    "lodash": "^4.17.21",
    "mobx": "^6.9.0",
    "mobx-react-lite": "^3.4.3",
    "nanoid": "^4.0.2",
    "tsd": "^0.28.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielyogel/vertical.git"
  },
  "bugs": {
    "url": "https://github.com/danielyogel/vertical/issues"
  },
  "homepage": "https://github.com/danielyogel/vertical#readme"
}
