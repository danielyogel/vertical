{
  "name": "@daniely/vertical",
  "description": "Vertical Abstract Software",
  "author": "Daniel Yogel",
  "version": "0.8.2",
  "license": "MIT",
  "main": "./dist/vertical.umd.js",
  "module": "./dist/vertical.es.js",
  "exports": {
    ".": {
      "import": "./dist/vertical.es.js",
      "require": "./dist/vertical.umd.js"
    }
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "concurrently  --group  --raw \"vite\" \"tsc --noEmit --watch\" \"npm run ladle\" \"npm run test\"",
    "build": "concurrently --raw \"vite build\" \"npm run build:types\"",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build",
    "ladle": "npx ladle serve",
    "ladle:build": "npx ladle build --out dist-ladle",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "engines": {
    "node": ">=12"
  },
  "prettier": {
    "printWidth": 170,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "peerDependencies": {
    "mobx": "^6.6.0",
    "react": ">=16"
  },
  "devDependencies": {
    "@ladle/react": "^0.15.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.35",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "antd": "^4.20.6",
    "classnames": "^2.3.1",
    "concurrently": "7.2.1",
    "postcss": "^8.4.14",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "ts-toolbelt": "9.6.0",
    "tslib": "^2.4.0",
    "type-fest": "2.12.2",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vitest": "^0.12.9"
  },
  "dependencies": {
    "fp-ts": "2.12.1",
    "io-ts": "^2.2.16",
    "lodash": "^4.17.21",
    "mobx": "^6.6.0",
    "mobx-react-lite": "^3.4.0",
    "nanoid": "^3.3.4",
    "tsd": "^0.20.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielyogel/vertical.git"
  },
  "bugs": {
    "url": "https://github.com/danielyogel/vertical/issues"
  },
  "homepage": "https://github.com/danielyogel/vertical#readme"
}
